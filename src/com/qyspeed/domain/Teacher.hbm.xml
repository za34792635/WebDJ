<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
    
    <class name="com.qyspeed.domain.Teacher" table="teacher">
        
        <id name="teacherNumber" column="teacherNumber" type="string" length="20">
            <generator class="assigned">
         </generator>
         </id>
         
         <property 
         name="teacherName" 
         column="teacherName" 
         type="string" 
         length="12"
         />
        <property 
        name="teacherPassword" 
        column="teacherPassword" 
        type="string" 
        length="30"
        />
        
         </class>
         
    </hibernate-mapping>>